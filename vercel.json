{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "includeFiles": ["ephe/**"],
        "nodeVersion": "18.x",
        "distDir": ".next",
        "forceBuildInContainer": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/",
      "dest": "/pages/index.js",
      "methods": ["GET"]
    }
  ]
}
